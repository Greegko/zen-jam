{
  "compilerOptions": {
    "target": "es5",
    "moduleResolution": "node",
    "sourceMap": true,
    "removeComments": false,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "noUnusedLocals": true,
    "noImplicitAny": false
  },
  "paths": {
    "*": [
      "node_modules/*",
      "src/*"
    ]
  },
}